{
  "compilerOptions": {
    "target": "ESNext",
    "moduleResolution": "Node16",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "module": "Node16",
    "noEmit": true,
    "allowImportingTsExtensions": true
  },
  "exclude": ["node_modules"]
}
